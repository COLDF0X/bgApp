<?xml version='1.0' encoding='utf-8'?>

<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>MyBoardGameApp</name>
    <description>
        An app to keep track of your board games and make suggestions on what games you can play.
    </description>
    <author email="andrewfoxba@hotmail.com" href="https://jose.cs.herts.ac.uk/af16acf/public/">
        Andrew Fox
    </author>
	<platform name="android" />
    <content src="index.html" />
    <access origin="*" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" />
    <plugin name="cordova-plugin-camera" spec="2.4.1" />
    <engine name="browser" spec="~5.0.3" />
    <engine name="android" spec="~7.0.0" />
</widget>
